
<table>
  <tr>
    <th>To</th>
    <th>From</th>
    <th>Amount</th>
  </tr>

<% @transactions.each do |transaction| %>
  <tr>
    <td><%= transaction.to %></td>
    <td><%= transaction.from %></td>
    <td><%= transaction.amount %></td>
  </tr>
<% end %>
</table>

